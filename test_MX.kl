PROGRAM Test_MX
-------------------------------------------------------------------------------
%ENVIRONMENT REGOPE

VAR
STS, i,j : INTEGER 
A1, A2, A3 : ARRAY [3,3] OF REAL


ROUTINE MUL_MX(M1, M2 : ARRAY[*,*] OF REAL): ARRAY[*,*] OF REAL FROM Matrix_lib
ROUTINE det_MX(A : ARRAY[*,*] OF REAL):  REAL FROM Matrix_lib
ROUTINE fill_MX(A11, A12, A13, A21, A22, A23, A31, A32, A33 :  REAL)  : ARRAY[*,*] OF REAL FROM Matrix_lib
ROUTINE kramer(A, D: ARRAY [*,*] OF REAL)  : ARRAY[*,*] OF REAL FROM Matrix_lib
-------------------------------------------------------------------------------
BEGIN

	A1 = fill_MX( 2,  5,  4,
				  1,  3,  2,
				  2, 10,  9)
	
	A2 = fill_MX( 30,  0,   0,
				  150,  0,   0,
				  110,  0,  0)

	A3 = kramer(a1,a2)
	
	
	WRITE('M1',CR)
	FOR i=1 TO 3 DO
		WRITE(a1[i,1]::3::1,		a1[i,2]::3::1,		a1[i,3]::3::1,		'    ',a2[i,1]::3::1,a2[i,2]::3::1,a2[i,3]::3::1)
		WRITE(CR)
	ENDFOR


	WRITE('M3',CR)
	FOR i=1 TO 3 DO
		FOR j=1 TO 3 DO
			WRITE(a3[i,j]::3::1)
		ENDFOR
		WRITE(CR)
	ENDFOR
	
	WRITE(det_Mx(A1)::8::2 )
END Test_MX
